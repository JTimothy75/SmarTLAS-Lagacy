  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"ng build --aot --prod --watch --no-delete-output-path\" \"node server.js\"",
    "start:dev": "concurrently --kill-others \"ng build --watch --no-delete-output-path\" \"nodemon server.js\"",
    "start:prod": "concurrently --kill-others \"ng build --aot --prod --watch --no-delete-output-path\" \"SET NODE_ENV=production&nodemon server.js\"",
    "start:dev2": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng serve -c=dev3 --port=4200 --open --ssl",
